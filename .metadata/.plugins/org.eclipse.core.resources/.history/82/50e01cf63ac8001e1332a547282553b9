package com.br.heranca.exercicio1;

public class Conta {

	public static void main(String[] args) {
		
		Cliente cliente1 = new Cliente("Rafa", "0912313", "Rua da paz", 30, "@rafa", 'F');
		ContaBancaria c1 = new ContaBancaria("Jose", 0001, 500);
		ContaEspecial c2 = new ContaEspecial("Joao", 0002, 1000, 500);
		ContaPoupanca c3 = new ContaPoupanca("Julia", 0003, 300);
		
		c1.sacar(600);
		c2.depositar(1500);
		c2.sacar(100000);
		c2.mostrarDadosDaConta();
		c3.mostrarDadosDaConta();
		
	}
}
