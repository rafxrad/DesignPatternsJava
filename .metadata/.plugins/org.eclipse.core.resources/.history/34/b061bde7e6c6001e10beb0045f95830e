
public class UtilitarioContas {
	
	public void imprimirContas (Conta[] contas) {
		for(int i = 0; i < contas.length; i++) {
			if (contas[i] != null) {
			System.out.println("Número da conta" + contas[i].getNumero());
		}
	}
}
	
	public void somarSaldoContas (Conta[] contas) {
		float saldoTotal = 0.0f;
		
		for(int i = 0; i < contas.length; i++) {
			if (contas[i] != null) {
				saldoTotal = saldoTotal + contas[i].getSaldo();
				System.out.println("Saldo total é: " + saldoTotal);
			}
		}
	}

}


