package com.br.heranca.exercicio1;

import static org.junit.Assert.*;

import org.junit.Test;

public class ContaBancariaTest {

	Cliente cliente1 = new Cliente("Rafa", "0912313", "Rua da paz", 30, "@rafa", 'F');
	
	
	@Test
	public void testaObjetoContaBancaria() {
		ContaBancaria c1 = new ContaBancaria(cliente1, 0001, 500);
		assertEquals("Jose", c1.getCliente());
		assertEquals(0001, c1.getNumConta());
	}

}
  