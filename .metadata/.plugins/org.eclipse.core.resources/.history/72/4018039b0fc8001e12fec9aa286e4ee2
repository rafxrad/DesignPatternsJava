
public class Gerente extends Funcionario{
	public Gerente(String nome, String cpf, float salario) {
		super(nome, cpf, salario);
		// TODO Auto-generated constructor stub
	}

	private String senha;
	private int tempoEmpresa;  
	
	
	
	public String getSenha() {
		return senha;
	}
	public void setSenha(String senha) {
		this.senha = senha;
	}
	public int getTempoEmpresa() {
		return tempoEmpresa;
	}
	public void setTempoEmpresa(int tempoEmpresa) {
		this.tempoEmpresa = tempoEmpresa;
	}
	
	@Override
	public void reajustarSalario(float percentual) {
		float bonus;
		bonus = tempoEmpresa / 2;
		super.reajustarSalario(percentual + bonus);
	}
}
